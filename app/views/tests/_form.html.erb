<%= simple_form_for [@test] do |f| %>
  <%= f.input :company %>
  <%= f.input :name %>
  <%= f.input :logo %>
  <%= f.input :logo_cache, as: :hidden %>
  <%= f.input :website %>
  <%= f.input :intro %>
  <%= f.input :description %>
  <%= f.input :review_price_cents %>
  <%= f.input :issue_price_cents %>
  <%= f.button :submit, class: "btn btn-primary" %>
<% end %>
