<div class="container text-center form-container">
	
  <h3>Add a new review</h3>
	  <%= simple_form_for [@test, @review] do |f| %>
	  <%= f.association :user %>
	  <%= f.input :user_agent %>
	  <%= f.input :issue_description %>
	  <%= f.input :issue_score %>
	  <%= f.input :image %>
	  <%= f.button :submit, class: "btn btn-primary" %>
	<% end %>
</div>